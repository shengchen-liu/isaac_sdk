{
  "name": "my_goal_generator",
  "modules": [
    "//apps/tutorials/webinar_1:go_to_mockup",
    "//apps/tutorials/webinar_1:my_goal_generator"
  ],
  "graph": {
    "nodes": [
      {
        "name": "go_to_mockup",
        "components": [
          {
            "name": "message_ledger",
            "type": "isaac::alice::MessageLedger"
          },
          {
            "name": "GoToMockup",
            "type": "isaac::tutorials::GoToMockup"
          }
        ]
      },
      {
        "name": "my_node",
        "components":[
          {
            "name": "message_ledger",
            "type": "isaac::alice::MessageLedger"
          },
          {
            "name": "MyComponent",
            "type": "isaac::tutorials::MyGoalGenerator"
          }
        ]
      }
    ],
    "edges": [
      {
        "source": "my_node/MyComponent/my_goal",
        "target": "go_to_mockup/GoToMockup/goal"
      }
    ]
  },
  "config": {
    "go_to_mockup" : {
      "GoToMockup" : {
        "tick_period" : "10ms"
      }
    },
    "my_node":{
      "MyComponent":{
        "tick_period" : "3s"
      }
    }
  }
}
