        "map_x_min": -10.0,
        "map_y_max": 10.0,
        "map_y_min": -10.0,
        "map_resolution": 0.05,
        "max_range": 85.5,
        "map_update_range": 85.0,
        "number_particles": 50,
        "linear_distance": 0.2,
        "angular_distance": 0.08,
        "use_pose_tree": false
      }
    },
    "recorder": {
        "isaac.alice.Recorder": {
        "base_directory": "/tmp/isaac",
        "enabled": false
      }
    },
    "recorder_bridge": {
      "RecorderBridge": {
        "recorder_component_name": "recorder/isaac.alice.Recorder",
        "tick_period": "1.0"
      }
    },
    "websight": {
      "WebsightServer": {
        "port": 3000,
        "ui_config": {
          "windows": {
