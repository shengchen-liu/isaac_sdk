{
  "name": "navigate",
  "modules": [
    "viewers"
  ],
  "graph": {
    "nodes": [
      {
        "name": "simulation",
        "subgraph": "packages/navsim/apps/navsim.subgraph.json"
      },
      {
        "name": "navigation",
        "subgraph": "packages/navigation/apps/differential_base_navigation.subgraph.json"
      },
      {
        "name": "goals",
        "subgraph": "packages/navigation/apps/goal_generators.subgraph.json"
      },
      {
        "name": "minisight",
        "subgraph": "packages/navsim/apps/minisight.subgraph.json"
      },
      {
        "name": "camera_viewer",
        "components": [
          {
            "name": "ledger",
            "type": "isaac::alice::MessageLedger"
          },
          {
            "name": "color_viewer",
            "type": "isaac::viewers::ColorCameraViewer"
          },
          {
            "name": "depth_viewer",
            "type": "isaac::viewers::DepthCameraViewer"
          }
        ]
      },
      {
        "name": "behavior",
        "components": [
          {
            "name": "NodeGroup",
            "type": "isaac::alice::behaviors::NodeGroup"
          },
          {
            "name": "MemorySequenceBehavior",
            "type": "isaac::alice::behaviors::MemorySequenceBehavior"
          }
        ]
      }
    ],
    "edges": [
      {
        "source": "navigation.subgraph/interface/feedback",
        "target": "goals.subgraph/interface/feedback"
      },
      {
        "source": "goals.subgraph/interface/goal",
        "target": "navigation.subgraph/interface/goal"
      },
      {
        "source": "simulation.interface/output/flatscan",
        "target": "navigation.subgraph/interface/flatscan_for_localization"
      },
      {
        "source": "simulation.interface/output/flatscan",
        "target": "navigation.subgraph/interface/flatscan_for_obstacles"
      },
      {
        "source": "simulation.interface/output/base_state",
        "target": "navigation.subgraph/interface/state"
      },
      {
        "source": "simulation.interface/output/imu_raw",
        "target": "navigation.subgraph/interface/imu_raw"
      },
      {
        "source": "navigation.subgraph/interface/command",
        "target": "simulation.interface/input/base_command"
      },
      {
        "source": "simulation.interface/output/color",
        "target": "camera_viewer/color_viewer/color_listener"
      },
      {
        "source": "simulation.interface/output/depth",
        "target": "camera_viewer/depth_viewer/depth_listener"
      },
      {
        "source": "navigation.planner.global_plan_smoother/smoother/smooth_plan",
        "target": "minisight.interface/input/global_plan"
      },
      {
        "source": "navigation.control.lqr/isaac.planner.DifferentialBaseLqrPlanner/plan",
        "target": "minisight.interface/input/local_plan"
      }
    ]
  },
  "config": {
    "behavior": {
      "MemorySequenceBehavior": {
        "tick_period": "10Hz"
      },
      "NodeGroup": {
        "node_names": [
          "simulation.scenario_manager",
          "navigation.localization.localize"
        ],
        "tick_period": "10Hz"
      }
    },
    "navigation.localization.localize": {
      "disable_automatic_start": true
    },
    "goals.goal_behavior": {
      "isaac.navigation.SelectorBehavior": {
        "desired_behavior": "pose"
      }
    },
    "navigation.imu_odometry.odometry": {
      "DifferentialBaseWheelImuOdometry": {
        "use_imu": false
      }
    },
    "goals.pose_as_goal": {
      "pose_as_goal_frame": {
        "lhs_frame": "unity"
      }
    },
    "camera_viewer": {
      "color_viewer": {
        "target_fps": 20,
        "reduce_scale": 4
      },
      "depth_viewer": {
        "colormap": [
          [ 128, 0, 0 ],
          [ 255, 0, 0 ],
          [ 255, 255, 0 ],
          [ 0, 255, 255 ],
          [ 0, 0, 255 ],
          [ 0, 0, 128 ]
        ],
        "min_visualization_depth": 0.0,
        "max_visualization_depth": 20.0,
        "target_fps": 20,
        "reduce_scale": 4
      }
    },
    "websight": {
      "WebsightServer": {
        "port": 3000,
        "ui_config": {
          "windows": {
            "navigate - Color Camera": {
              "renderer": "2d",
              "channels": [
                {
                  "name": "navigate/camera_viewer/color_viewer/Color"
                }
              ]
            },
            "navigate - Depth Camera": {
              "renderer": "2d",
              "channels": [
                {
                  "name": "navigate/camera_viewer/depth_viewer/Depth"
                }
              ]
            },
            "navigate - 3D View": {
              "channels": [
                {
                  "name": "navigate/map/occupancy/map"
                },
                {
                  "name": "navigate/navigation.local_map.local_map/isaac.navigation.LocalMap/local_map"
                },
                {
                  "name": "navigate/navigation.planner.global_plan/isaac.planner.GlobalPlanner/target"
                },
                {
                  "name": "navigate/navigation.planner.global_plan_smoother/smoother/smooth_plan.path"
                },
                {
                  "name": "navigate/navigation.control.lqr/isaac.planner.DifferentialBaseLqrPlanner/plan"
                },
                {
                  "name": "navigate/navigation.localization.scan_localization/robot_viewer/robot"
                }
              ],
              "dims": {
                "height": 480,
                "width": 640
              },
              "renderer": "3d"
            },
            "navigate - Local View": {
              "channels": [
                {
                  "name": "navigate/navigation.local_map.local_map/isaac.navigation.LocalMap/local_map"
                },
                {
                  "name": "navigate/navigation.planner.global_plan/isaac.planner.GlobalPlanner/target"
                },
                {
                  "name": "navigate/navigation.planner.global_plan_smoother/smoother/smooth_plan.path"
                },
                {
                  "name": "navigate/navigation.localization.scan_localization/robot_viewer/robot"
                },
                {
                  "name": "navigate/navigation.control.lqr/isaac.planner.DifferentialBaseLqrPlanner/plan"
                }
              ],
              "dims": {
                "height": 256,
                "width": 256
              },
              "renderer": "2d"
            },
            "navigate - Map View": {
              "channels": [
                {
                  "name": "navigate/map/occupancy/map"
                },
                {
                  "name": "navigate/map/waypoints/waypoints"
                },
                {
                  "name": "navigate/navigation.local_map.local_map/isaac.navigation.LocalMap/local_map"
                },
                {
                  "name": "navigate/navigation.control.lqr/isaac.planner.DifferentialBaseLqrPlanner/plan"
                },
                {
                  "name": "navigate/navigation.localization.scan_localization/flatscan_viewer/beam_lines"
                },
                {
                  "name": "navigate/navigation.localization.scan_localization/flatscan_viewer/beam_endpoints"
                },
                {
                  "name": "navigate/navigation.planner.global_plan/isaac.planner.GlobalPlanner/target"
                },
                {
                  "name": "navigate/navigation.planner.global_plan_smoother/smoother/smooth_plan.path"
                },
                {
                  "name": "navigate/navigation.localization.scan_localization/robot_viewer/robot"
                }
              ],
              "dims": {
                "height": 548,
                "width": 432
              },
              "renderer": "2d"
            },
            "navigate - Speed profile": {
              "channels": [
                {
                  "name": "navigate/navigation.control.control/isaac.planner.DifferentialBaseControl/controller.commanded_speed"
                },
                {
                  "name": "navigate/navigation.imu_odometry.odometry/DifferentialBaseWheelImuOdometry/dynamics.observed_base_speed"
                },
                {
                  "name": "navigate/navigation.control.control/isaac.planner.DifferentialBaseControl/controller.commanded_angular_speed"
                },
                {
                  "name": "navigate/navigation.imu_odometry.odometry/DifferentialBaseWheelImuOdometry/dynamics.observed_base_angular_speed"
                }
              ],
              "dims": {
                "height": 480,
                "width": 640
              },
              "renderer": "plot"
            },
            "navigate - IMU": {
              "renderer": "plot",
              "channels": [
                {
                  "name": "navigate/navigation.imu_odometry.imu_corrector/ImuCorrector/RawAngularVelocity.z"
                },
                {
                  "name": "navigate/navigation.imu_odometry.imu_corrector/ImuCorrector/CorrectedAngularVelocity.z"
                },
                {
                  "name": "navigate/navigation.imu_odometry.imu_corrector/ImuCorrector/RawLinearAcceleration.x"
                },
                {
                  "name": "navigate/navigation.imu_odometry.imu_corrector/ImuCorrector/CorrectedLinearAcceleration.x"
                },
                {
                  "name": "navigate/navigation.imu_odometry.imu_corrector/ImuCorrector/RawLinearAcceleration.y"
                },
                {
                  "name": "navigate/navigation.imu_odometry.imu_corrector/ImuCorrector/CorrectedLinearAcceleration.y"
                }
              ],
              "dims": {
                "height": 480,
                "width": 640
              }
            }
          }
        }
      }
    }
  }
}
