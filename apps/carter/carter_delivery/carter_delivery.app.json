      },
      {
        "source": "carter_delivery/carter_delivery/target_waypoint",
        "target": "waypoint_as_goal/isaac.navigation.MapWaypointAsGoal/desired_waypoint"
      },
      {
        "source": "navigation.subgraph/interface/feedback",
        "target": "carter_delivery/carter_delivery/goal_feedback"
      },
      {
        "source": "waypoint_as_goal/isaac.navigation.MapWaypointAsGoal/goal",
        "target": "navigation.subgraph/interface/goal"
      }
    ]
  },
  "config": {
    "carter_delivery": {
      "carter_delivery": {
        "tick_period": "10ms",
        "popcorn_pickup_waypoint": "kitchen",
        "navigation_mode": "navigation.control.navigation_mode/isaac.navigation.GroupSelectorBehavior"
      }
    },
    "navigation.control.lqr": {
      "isaac.planner.DifferentialBaseLqrPlanner": {
        "manual_mode_channel": "commander.robot_remote/isaac.navigation.RobotRemoteControl/manual_mode"
      }
    },
    "navigation.control.control": {
      "isaac.planner.DifferentialBaseControl": {
        "manual_mode_channel": "commander.robot_remote/isaac.navigation.RobotRemoteControl/manual_mode"
      }
    },
    "slackbot": {
      "slackbot": {
        "bot_token": "bot_token_here"
      }
    },
    "waypoint_as_goal": {
      "isaac.navigation.MapWaypointAsGoal": {
        "map_layer": "map/waypoints",
        "tick_period": "50ms"
      }
    },
    "websight": {
      "WebsightServer": {
        "port": 3000,
        "ui_config": {
          "windows": {
            "Carter Delivery - Local View": {
              "renderer": "2d",
              "dims": {
                "width": 256,
                "height": 256
              },
              "channels": [
                { "name": "carter_delivery/navigation.local_map.local_map/isaac.navigation.LocalMap/local_map" },
                { "name": "carter_delivery/navigation.localization.scan_localization/robot_viewer/robot" },
                { "name": "carter_delivery/navigation.control.lqr/isaac.planner.DifferentialBaseLqrPlanner/plan" },
                { "name": "carter_delivery/navigation.planner.global_plan/isaac.planner.GlobalPlanner/path" }
              ]
            },
            "Carter Delivery - Map View": {
              "renderer": "2d",
              "dims": {
                "width": 432,
                "height": 548
              },
              "channels": [
                { "name": "carter_delivery/map/occupancy/map" },
                { "name": "carter_delivery/map/waypoints/waypoints" },
                { "name": "carter_delivery/navigation.localization.scan_localization/flatscan_viewer/beam_lines" },
                {
                  "name": "carter_delivery/map/restricted_area/polygons",
                  "active": false
                },
                {
                  "name": "carter_delivery/navigation.planner.global_plan/isaac.planner.GlobalPlanner/graph",
                  "active": false
                },
                { "name": "carter_delivery/navigation.planner.global_plan/isaac.planner.GlobalPlanner/target" },
                { "name": "carter_delivery/navigation.planner.global_plan/isaac.planner.GlobalPlanner/path" },
                {
                  "name": "carter_delivery/navigation.localization.global_localization/grid_search_localizer/error",
                  "active": false
                },
                { "name": "carter_delivery/navigation.localization.scan_localization/robot_viewer/robot" },
                {
                  "name": "carter_delivery/navigation.localization.scan_localization/flatscan_viewer/beam_endpoints",
                  "active": true
                },
                {
                  "name": "carter_delivery/navigation.localization.scan_localization/isaac.navigation.ParticleFilterLocalization/particles",
                  "active": true
                }
              ]
            }
          }
        }
      }
    }
  }
